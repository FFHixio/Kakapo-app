{
  "name": "kakapo-app",
  "version": "1.3.0",
  "description": "A background noise app",
  "main": "browser.js",
  "repository": "git+https://github.com/bluedaniel/Kakapo-app.git",
  "author": "Daniel Levitt @supercerebral",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bluedaniel/Kakapo-app/issues"
  },
  "homepage": "https://github.com/bluedaniel/Kakapo-app#readme",
  "scripts": {
    "build": "babel-node tools/run build",
    "clean": "babel-node tools/run clean",
    "eslint-check": "eslint --print-config .eslintrc | eslint-config-prettier-check",
    "installer-mac": "babel-node tools/run installer-mac",
    "installer-win": "babel-node tools/run installer-win",
    "lint": "stylelint \"app/scripts/**/*.css\" && eslint '{__tests__,app/scripts}/**/*.js'",
    "pkg": "babel-node tools/run pkg",
    "start": "babel-node tools/run start",
    "styles:format": "stylefmt -r \"app/**/*.css\"",
    "test": "npm run lint && cross-env NODE_ENV=test jest",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "automock": false,
    "setupFiles": [
      "./jest/setup.js",
      "jest-localstorage-mock"
    ],
    "testMatch": [
      "**/__tests__/**/*.spec.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js",
      "^kakapoBridge$": "<rootDir>/app/scripts/bridge/web/index.js"
    },
    "collectCoverage": false,
    "collectCoverageFrom": [
      "app/scripts/**/*.{js,jsx}",
      "!app/scripts/bridge/desktop/**",
      "!app/scripts/*.js"
    ]
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "clipboard": "^2.0.4",
    "connected-react-router": "^5.0.1",
    "fs-extra": "^7.0.1",
    "hint.css": "^2.5.1",
    "history": "^4.7.2",
    "howler": "^2.0.15",
    "intl": "^1.2.4",
    "kakapo-assets": "^5.18.0",
    "lost": "^8.3.0",
    "polished": "^2.3.1",
    "query-string": "^6.2.0",
    "ramda": "^0.26.1",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-dropzone": "^7.0.1",
    "react-hot-loader": "^4.3.12",
    "react-intl": "^2.7.2",
    "react-redux": "^5.1.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-transition-group": "^2.5.0",
    "recompose": "^0.30.0",
    "redux": "^4.0.1",
    "redux-saga": "^0.16.2",
    "remote-redux-devtools": "^0.5.13",
    "request": "^2.88.0",
    "semver": "^5.6.0",
    "shortid": "^2.2.14",
    "whatwg-fetch": "^3.0.0",
    "ytdl-core": "^0.28.0"
  },
  "devDependencies": {
    "archiver": "^3.0.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.4",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "compression-webpack-plugin": "^2.0.0",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.1",
    "cssnano": "^4.1.7",
    "devtron": "^1.4.0",
    "electron": "^3.0.10",
    "electron-devtools-installer": "^2.2.4",
    "electron-packager": "^12.2.0",
    "electron-winstaller": "^2.7.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "exports-loader": "^0.7.0",
    "express": "^4.16.4",
    "husky": "^1.2.0",
    "imagemin": "^6.0.0",
    "imagemin-jpegtran": "^6.0.0",
    "imagemin-pngquant": "^6.0.0",
    "imports-loader": "^0.8.0",
    "jest": "^23.6.0",
    "jest-fetch-mock": "^2.0.1",
    "jest-localstorage-mock": "^2.3.0",
    "lint-staged": "^8.1.0",
    "mini-css-extract-plugin": "^0.4.5",
    "null-loader": "^0.1.1",
    "open": "0.0.5",
    "ora": "^3.0.0",
    "postcss": "^7.0.6",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.1.0",
    "postcss-preset-env": "^6.4.0",
    "postcss-reporter": "^6.0.0",
    "postcss-url": "^8.0.0",
    "prettier": "^1.15.3",
    "raf": "^3.4.1",
    "rcedit": "^1.1.1",
    "react-test-renderer": "^16.6.3",
    "redux-mock-store": "^1.5.3",
    "style-loader": "^0.23.1",
    "stylefmt": "^6.0.3",
    "stylelint": "^9.9.0",
    "stylelint-config-standard": "^18.2.0",
    "webpack": "^4.26.1",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-hot-middleware": "^2.24.3",
    "yargs": "^12.0.5"
  }
}
